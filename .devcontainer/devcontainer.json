{
  "dockerComposeFile": ["../docker-compose.yaml"],
  "service": "app" ,
  "workspaceFolder": "/workspace",
  "containerEnv": {
    "TZ": "Asia/Tokyo"
  },
  "customizations": {
		"vscode": {
      "settings": {
        "go.editor.formatOnSave": false,
        "go.editor.codeActionsOnSave": {
          "source.organizeImports": false
        }
      },
			"extensions": [
        "golang.go"
      ]
    }
  },
  "shutdownAction": "none"
}